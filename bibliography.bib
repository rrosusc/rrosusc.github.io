@article{Yao2005,
abstract = {In this paper, we address the path planning problem with general end-effector$\backslash$nconstraints (PPGEC) for robot manipulators. Two approaches are proposed.$\backslash$nThe first approach is adapted from an existing randomized gradient$\backslash$ndescent (RGD) method for closed-chain robots. The second approach$\backslash$nis radically different. We call it ATACE alternate task-space and$\backslash$nconfiguration-space exploration. Unlike the first approach which$\backslash$nsearches purely in C-space, ATACE works in both task space and C-space.$\backslash$nIt explores the task space for end-effector paths satisfying given$\backslash$nconstraints, and utilizes trajectory tracking technique(s) as a local$\backslash$nplanner(s) to track these paths in the configuration space. We have$\backslash$nimplemented both approaches and compare their relative performances$\backslash$nin different scenarios. ATACE outperforms RGD in majority (but not$\backslash$nall) of the scenarios. We outline intuitive explanations for the$\backslash$nrelative performances of these two approaches.},
author = {Yao, Zhenwang and Gupta, Kamal},
doi = {10.1109/IROS.2005.1545305},
file = {:home/shantanu/Desktop/Proposal Citations/Journals/Path planning with general end-effector constraints.pdf:pdf},
