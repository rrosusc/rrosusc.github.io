@article{Dietrich2012,
abstract = {As a result of intensive research over the last few decades, several$\backslash$nrobotic systems are approaching a level of maturity that allows robust$\backslash$ntask execution and safe interaction with humans and the environment.$\backslash$nParticularly when considering the aging of the population, service$\backslash$nand household robotics is expected to play an important role in future$\backslash$ndomestic environments. To provide the ability to accomplish a huge$\backslash$nrange of tasks with different requirements, it appears to be inevitable$\backslash$nto equip the robot with a large number of degrees of freedom (DoF).$\backslash$nJust imagine an ostensibly simple service task like filling a glass$\backslash$nwith water and placing it on a table. A variety of constraints has$\backslash$nto be dealt with simultaneously: No liquid should be slopped, collisions$\backslash$nwith the environment must be avoided, and possible interactions with$\backslash$nhumans residing in the workspace of the robot have to be handled$\backslash$nproperly.},
